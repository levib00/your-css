{
  "manifest_version": 2,
  "name": "Your Css",
  "version": "1.0",

  "description": "Add and save your own custom css for whatever websites you want.",

  "icons": {
    "48": "icons/border-48.png"
  },
  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "your-css",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"]
    }
  ]
}
